def getMoneySpent(k,d,b):
    spend = -1
    for i in k:
        for j in d:
            if i + j <=b:
                spend = max(spend, i + j)
    return spend
b,n,m = map(int,input().split())
keyboard = list(map(int,input().split()))
drives = list(map(int,input().split()))
print(getMoneySpent(keyboard,drives,b))
